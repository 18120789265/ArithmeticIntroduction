
public class paixu {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = {1,3,5,2,6};
		insert(arr);
	}
	
	//1,3,5    2,6
	//
	public static void insert(int[] arr) {
		int t = 0;
		int v = -1;
		for (int i = 1; i < arr.length; i++) {
			t = arr[i];
			for (int j = i; j >= 0; j--) {
				if (arr[j] > t) {
					arr[j+1] = arr[j];
					v = j;
				}
			}
			if(v != -1){
				arr[v] = t;
			}
		}
		for (int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}
	}

}
